---
import Welcome from '../../components/Welcome.astro';
import Layout from '../../layouts/Layout.astro';
import Hero from '../../components/react/Hero2';
import EventSchedule from '../../components/react/EventSchedule';
import EmotionalPain from '../../components/react/EmotionalPain';
import ClassBenefits from '../../components/react/ClassBenefits';
import Speaker from '../../components/react/Speaker';
import WhatYouWillLearn from '../../components/react/WhatYouWillLearn';
import Testimonials from '../../components/react/Testimonials';
import ActionPlan from '../../components/react/ActionPlan';
import CallToAction from '../../components/react/CallToAction';
---

<Layout>
    <Hero client:load backgroundImage="/images/bg/bg.webp" />
    <EventSchedule client:idle />
    <EmotionalPain client:idle />
    <WhatYouWillLearn client:idle />
    <ClassBenefits client:idle />
    <Speaker client:idle />
    <Testimonials client:idle />
    <ActionPlan client:idle />
    <CallToAction client:idle />
</Layout>
